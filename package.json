{
  "name": "anc-audio-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev:turbo": "next dev --port 3000",
    "dev:profile": "cross-env NODE_OPTIONS='--inspect --max-old-space-size=4096' next dev",
    "build": "rimraf .next && next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:optimize": "node scripts/build-optimize.js",
    "build:production": "npm run clean && npm run lint && npm run type-check && npm run build:optimize",
    "start": "next start",
    "start:production": "cross-env NODE_ENV=production next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "deploy:production": "npm run build:production && vercel --prod",
    "deploy:preview": "vercel",
    "docker:build": "docker build -t anc-audio-app .",
    "docker:run": "docker run -p 3000:3000 anc-audio-app",
    "docker:production": "docker-compose -f docker-compose.production.yml up -d",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "mobile:build": "cross-env CAPACITOR_BUILD=true next build",
    "mobile:sync": "npm run mobile:build && npx cap sync",
    "mobile:android": "npm run mobile:sync && npx cap open android",
    "mobile:ios": "npm run mobile:sync && npx cap open ios",
    "mobile:run-android": "npm run mobile:sync && npx cap run android",
    "mobile:run-ios": "npm run mobile:sync && npx cap run ios",
    "clean": "rimraf .next out node_modules/.cache .turbo",
    "clean:all": "rimraf .next out node_modules node_modules/.cache .turbo && npm install",
    "clean:cache": "rimraf .next/cache .turbo node_modules/.cache",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "db:init": "tsx scripts/database/init-db.ts",
    "db:reset": "npm run db:init",
    "postinstall": "echo \"ðŸ“¦ Dependencies installed successfully!\""
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@clerk/nextjs": "^5.7.1",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@neondatabase/serverless": "^1.0.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@stripe/stripe-js": "^7.8.0",
    "@types/dom-speech-recognition": "^0.0.6",
    "@types/validator": "^13.15.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "file-saver": "^2.0.5",
    "idb": "^8.0.3",
    "isomorphic-dompurify": "^2.27.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.541.0",
    "next": "^14.2.0",
    "next-themes": "^0.4.6",
    "onnxruntime-web": "^1.22.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-dropzone": "^14.3.8",
    "sonner": "^2.0.7",
    "stripe": "^18.4.0",
    "svix": "^1.74.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/testing-library__jest-dom": "^6.0.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
