{
  "name": "anc-audio-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' next dev --turbopack",
    "dev:turbo": "next dev --turbopack --port 3000",
    "dev:profile": "cross-env NODE_OPTIONS='--inspect --max-old-space-size=4096' next dev --turbopack",
    "build": "next build --turbopack",
    "build:analyze": "cross-env ANALYZE=true next build --turbopack",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next out node_modules/.cache .turbo",
    "clean:all": "rimraf .next out node_modules node_modules/.cache .turbo && npm install",
    "clean:cache": "rimraf .next/cache .turbo node_modules/.cache",
    "test": "echo \"No tests specified\" && exit 0",
    "postinstall": "echo \"ðŸ“¦ Dependencies installed successfully!\""
  },
  "dependencies": {
    "@clerk/nextjs": "^6.31.4",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@neondatabase/serverless": "^1.0.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@stripe/stripe-js": "^7.8.0",
    "@types/dom-speech-recognition": "^0.0.6",
    "@types/validator": "^13.15.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "file-saver": "^2.0.5",
    "idb": "^8.0.3",
    "isomorphic-dompurify": "^2.27.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "next-themes": "^0.4.6",
    "onnxruntime-web": "^1.22.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "sonner": "^2.0.7",
    "stripe": "^18.4.0",
    "svix": "^1.74.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "typescript": "^5"
