  "shortcuts": [
    {
      "name": "Upload Audio",
      "short_name": "Upload",
      "description": "Quickly upload and process audio files",
      "url": "/dashboard?tab=upload",
      "icons": [{ "src": "/icons/upload-shortcut.svg", "sizes": "96x96", "type": "image/svg+xml" }]
    },
    {
      "name": "AI Processor",
      "short_name": "Process",
      "description": "Access advanced AI audio processing",
      "url": "/dashboard?tab=processor",
      "icons": [{ "src": "/icons/process-shortcut.svg", "sizes": "96x96", "type": "image/svg+xml" }]
    },
    {
      "name": "Search Audio",
      "short_name": "Search",
      "description": "Search through your audio content",
      "url": "/dashboard?tab=search",
      "icons": [{ "src": "/icons/search-shortcut.svg", "sizes": "96x96", "type": "image/svg+xml" }]
    }
  ],
  "share_target": {
    "action": "/dashboard?share=true",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "audio",
          "accept": ["audio/*", "video/*"]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/dashboard?handler=audio",
      "accept": {
        "audio/*": [".mp3", ".wav", ".m4a", ".aac", ".ogg", ".flac"],
        "video/*": [".mp4", ".webm", ".avi", ".mov", ".mkv"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+ancaudio",
      "url": "/dashboard?audio=%s"
    }
  ],
  "prefer_related_applications": false,
  "related_applications": [],
  "edge_side_panel": {
    "preferred_width": 400
  }
}